{
  "compilerOptions": {
	"target": "es2020",
	"useDefineForClassFields": true,
	"lib": [
	  "DOM",
	  "DOM.Iterable",
	  "es2020",
	  "esnext"
	],
	"allowJs": false,
	"skipLibCheck": false,
	"esModuleInterop": false,
	"allowSyntheticDefaultImports": true,
	"strict": true,
	"forceConsistentCasingInFileNames": true,
	"module": "ESNext",
	"moduleResolution": "Node",
	"resolveJsonModule": true,
	"isolatedModules": true,
	"noEmit": true,
	"jsx": "react-jsx",
	"types": [
	  "react/next",
	  "react-dom/next"
	]
  },
  "include": [
	"./src"
  ]
}
